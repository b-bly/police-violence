{
    "name": "server",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "tsc": "tsc",
        "test": "npx jest",
        "clean": "rimraf dist/*",
        "lint": "npx tslint -c tslint.json -p tsconfig.json --fix",
        "prebuild": "npm run clean && npm run lint",
        "build": "npm run tsc",
        "postbuild": "cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --no-overwrite --parents",
        "start": "NODE_ENV=production && node ./dist/index.js",
        "dev:start": "ts-node src/index.ts",
        "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start"
    },
    "devDependencies": {
        "@jest/globals": "^29.3.1",
        "@types/expect": "^24.3.0",
        "@types/express": "^4.17.7",
        "@types/fs-extra": "^9.0.1",
        "@types/moment": "^2.13.0",
        "@types/node": "^14.0.22",
        "@types/node-fetch": "^2.5.7",
        "@types/shelljs": "^0.8.8",
        "@types/testing-library__dom": "^7.5.0",
        "cpy-cli": "^2.0.0",
        "del-cli": "^3.0.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.1.5",
        "tslint": "^6.1.2",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "csv-parser": "^2.3.3",
        "d3-fetch": "^1.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "lodash": "^4.17.19",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "node-fetch": "^2.6.0"
    }
}
